<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
  <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
  <script src="header_footer.js"></script>
  <title>AGREGAR ESTUDIANTES</title>
</head>

<body>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
    <%- include('partials/nav.ejs')%>
  </nav>
  <div eblclass="container">
    <a href="/create" class="btn btn-outline-primary mt-4"><i class='bx bxs-plus-circle bx-md'></i></a>
    <table class="table table-bordered table-striped text-center mt-4">
      <thead>
        <tr class="bg-primary text-white">
          <th scope="col">ID</th>
          <th scope="col">ESTUDIANTE</th>
          <th scope="col">DNI</th>
          <th scope="col">CIUDAD</th>
          <th scope="col">CELULAR</th>
          <th scope="col">CORREO</th>
          <th scope="col">ACCIONES</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach((estudiantes)=>{ %>
          <tr>
            <td>
              <%= estudiantes.id_estudiante%>
            </td>
            <td>
              <%= estudiantes.nombre%>
            </td>
            <td>
              <%= estudiantes.dni%>
            </td> 
            <td>
              <%= estudiantes.ciudad%>
            </td>
            <td>
              <%= estudiantes.numero_telefono%>
            </td>
            <td>
              <%= estudiantes.correo_electronico%>
            </td>
            <td>
              <a href="/edit/<%= estudiantes.id_estudiantes %>" class="btn btn-outline-info"><i
                  class='bx bxs-edit'></i></a>
              <a href="/delete/<%= estudiantes.id_estudiantes %>" class="btn btn-outline-danger"><i
                  class='bx bxs-trash-alt'></i></a>
            </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW"
    crossorigin="anonymous"></script>
</body>
<%-include ('partials/footer.ejs')%>

</html>